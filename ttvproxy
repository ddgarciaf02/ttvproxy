function FindProxyForURL(url, host) {
    var proxy = "PROXY 193.3.23.171:29032";
    
    // URL REGEX: ^https://usher.ttvnw.net/*/*/*/* - PROXY
    if (/^https:\/\/usher\.ttvnw\.net\/.*\/.*\/.*\/.*/.test(url)) {
        return proxy;
    }
    
    // URL REGEX: ^https://video-weaver.*.hls.ttvnw.net - PROXY
    if (/^https:\/\/video-weaver.*\.hls\.ttvnw\.net/.test(url)) {
        return proxy;
    }
    
    // Host wildcard: gql.twitch.tv - PROXY
    if (shExpMatch(host, "gql.twitch.tv")) {
        return proxy;
    }
    
    // Host wildcard: passport.twitch.tv - PROXY
    if (shExpMatch(host, "passport.twitch.tv")) {
        return proxy;
    }
    
    // Twitter
    var domainsuffix = [
    "ads-twitter.com",
    "cms-twdigitalassets.com",
    "periscope.tv",
    "pscp.tv",
    "t.co",
    "tellapart.com",
    "tweetdeck.com",
    "twimg.co",
    "twimg.com",
    "twimg.org",
    "twitpic.com",
    "twitter.biz",
    "twitter.com",
    "twitter.jp",
    "twittercommunity.com",
    "twitterflightschool.com",
    "twitterinc.com",
    "twitteroauth.com",
    "twitterstat.us",
    "twtrdns.net",
    "twttr.com",
    "twttr.net",
    "twvid.com",
    "vine.co"
  ];

  var domainkeyword = ["twitter"];

  var ipcidr = [
    "192.133.76.0/22",
    "199.16.156.0/22",
    "199.59.148.0/22",
    "199.96.56.0/21",
    "202.160.128.0/22",
    "209.237.192.0/19",
    "69.195.160.0/19"
  ];

  for (var i = 0; i < domainsuffix.length; i++) {
    if (dnsDomainIs(host, "." + domainsuffix[i])) {
      return proxy;
    }
  }

  for (var j = 0; j < domainkeyword.length; j++) {
    if (dnsDomainIs(host, domainkeyword[j])) {
      return proxy;
    }
  }

  for (var k = 0; k < ipcidr.length; k++) {
    if (isInNet(host, ipcidr[k])) {
      return proxy;
    }
  }
   
    // Default - Direct
    return "DIRECT";
}
