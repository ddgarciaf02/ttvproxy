function FindProxyForURL(url, host) {
  if (shExpMatch(url, "https://usher.ttvnw.net/*/*/*/*")) {
    return "PROXY 193.3.23.171:29032";
  }
  
  if (shExpMatch(url, "https://video-weaver.*.hls.ttvnw.net")) {
    return "PROXY 193.3.23.171:29032";
  }
  
  if (dnsDomainIs(host, "gql.twitch.tv")) {
    return "PROXY 193.3.23.171:29032";
  }
  
  if (dnsDomainIs(host, "passport.twitch.tv")) {
    return "PROXY 193.3.23.171:29032";
  }
  
  return "DIRECT";
}
